@model AdministracaoShrekPark.Models.FastFoodStore
@{
    ViewData["Title"] = "Detalhes";
}

<h1>Lanches: @Model.Id</h1>
<hr>
<p>Número de lanches: @Model.NumberOfBooths</p>
<p>É vegetariano?: @Model.ToiletPaper</p>
<p>Tem batat assada??: @Model.Mirror</p>
<p>Localização: @Model.Localization</p>
<a class="btn btn-primary btn-sm"asp-controller="FastFoodStore" asp-action="Delete" asp-route-id="@Model.Id">Delete</a>
<hr>